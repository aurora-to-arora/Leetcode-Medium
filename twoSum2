class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
        left=0
        right = len(numbers)-1
        seen = {}

        while left<=right:
            diff=numbers[left]+numbers[right]
            if diff == target:
                return [left+1,right+1]
            if diff>target:
                right-=1
            else:
                left+=1
        return []
